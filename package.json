{
  "name": "timed-automata-analysis",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "clean": "rimraf dist playwright-report test-results",
    "build": "tsc && vite build",
    "format": "prettier --write \"./**/*.{css,html,js,cjs,json,md,ts,tsx,yaml}\"",
    "lint": "eslint . --fix --report-unused-disable-directives",
    "test": "jest",
    "e2e": "playwright test",
    "e2e-ui": "playwright test --ui",
    "e2e-update-snapshots": "playwright test --update-snapshots",
    "deploy": "gh-pages -d dist/timed-automata-analysis",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.3.2",
    "@mui/material": "7.3.2",
    "i18next": "25.5.2",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-http-backend": "3.0.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-i18next": "15.7.3",
    "timed-automata-analyzer": "1.0.7",
    "vis-data": "8.0.3",
    "vis-network": "10.0.2"
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@playwright/test": "1.55.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.5.1",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@types/vis": "4.21.27",
    "@vitejs/plugin-react": "5.0.3",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-unused-imports": "4.2.0",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "jest-environment-jsdom": "30.1.2",
    "lint-staged": "16.1.6",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "ts-jest": "29.4.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0",
    "vite": "7.1.5",
    "vite-plugin-top-level-await": "1.6.0",
    "vite-plugin-wasm": "3.5.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --report-unused-disable-directives --max-warnings 0"
    ],
    "*.{css,html,js,cjs,json,md,yaml}": "prettier --write"
  }
}
