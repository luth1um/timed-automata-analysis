{
  "name": "timed-automata-analysis",
  "private": true,
  "version": "2.1.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "tsc && vite build",
    "format": "prettier --write \"./**/*.{css,html,js,cjs,json,md,ts,tsx,yaml}\"",
    "lint": "eslint . --fix --report-unused-disable-directives",
    "test": "jest",
    "e2e": "playwright test",
    "e2e-ui": "playwright test --ui",
    "e2e-update-snapshots": "playwright test --update-snapshots",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist/timed-automata-analysis",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.2.0",
    "@mui/material": "7.2.0",
    "i18next": "25.3.1",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-http-backend": "3.0.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "15.6.0",
    "timed-automata-analyzer": "1.0.4",
    "vis-data": "7.1.9",
    "vis-network": "9.1.13"
  },
  "devDependencies": {
    "@eslint/js": "9.30.1",
    "@playwright/test": "1.53.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.0.10",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/vis": "4.21.27",
    "@vitejs/plugin-react": "4.6.0",
    "eslint": "9.30.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-unused-imports": "4.1.4",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "jest": "30.0.4",
    "jest-environment-jsdom": "30.0.4",
    "lint-staged": "16.1.2",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "ts-jest": "29.4.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.1",
    "vite": "7.0.2",
    "vite-plugin-top-level-await": "1.5.0",
    "vite-plugin-wasm": "3.4.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --report-unused-disable-directives --max-warnings 0"
    ],
    "*.{css,html,js,cjs,json,md,yaml}": "prettier --write"
  }
}
