{
  "name": "timed-automata-analysis",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "tsc && vite build",
    "format": "prettier --write \"./**/*.{css,html,js,cjs,json,md,ts,tsx}\"",
    "lint": "eslint . --fix --report-unused-disable-directives",
    "test": "jest",
    "e2e": "playwright test",
    "e2e-ui": "playwright test --ui",
    "e2e-update-snapshots": "playwright test --update-snapshots",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist/timed-automata-analysis",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "11.13.0",
    "@emotion/styled": "11.13.0",
    "@mui/icons-material": "5.16.6",
    "@mui/material": "5.16.6",
    "i18next": "23.12.2",
    "i18next-browser-languagedetector": "8.0.0",
    "i18next-http-backend": "2.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "15.0.0",
    "vis-data": "7.1.9",
    "vis-network": "9.1.9"
  },
  "devDependencies": {
    "@playwright/test": "1.45.3",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.0.0",
    "@types/jest": "29.5.12",
    "@types/node": "22.1.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/vis": "4.21.27",
    "@vitejs/plugin-react": "4.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.9",
    "eslint-plugin-unused-imports": "4.0.1",
    "gh-pages": "6.1.1",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.8",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "ts-jest": "29.2.4",
    "typescript": "5.5.4",
    "typescript-eslint": "7.18.0",
    "vite": "5.3.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --report-unused-disable-directives --max-warnings 0"
    ],
    "*.{css,html,js,cjs,json,md}": "prettier --write"
  }
}
